page {
  background-image: url(http://mecs.ip165.com/WeiXin/Style/Image/login/bj.png);
  font-size: 20px;
  font-family: "微软雅黑";
  overflow: hidden;
  background-size: 100%;
}

image {
  border: none;
  vertical-align: top;
}

.content {
  width: 100%;
  margin: 60% 0% 0% 0%;
  position: absolute;
}

.check {
  width: 25px;
  height: 25px;
}

.content .ul .li  input {
  width: 50%;
  border: 0;
  background-color: transparent;
  font-size: 25px;
}

.content .ul .li image {
  vertical-align: middle;
  width: 25px;
  height: 25px;
}

.content .ul .li .btnsavecss {
  width: 115%;
  height: 45px;
  background-color: #06f;
  font-size: 25px;
  text-align: center;
}

p, h1, h2, h3, h4, dl, dd, input, textarea, select {
  padding: 0;
  margin: 0;
}

.content .ul .li  .imgMe {
  margin-left: 20%;
  width: 200px;
  height: 45px;
}

.li {
  padding-left: 10px;
  letter-spacing: 2px;
  width: 90%;
  height: 50px;
  text-align: left;
  list-style-type: none;
}
