 <cu-custom bgColor="bg-gradual-blue" isBack="{{true}}"><view slot="backText">返回</view><view slot="content">明细</view></cu-custom> 
<view class='ziview'>
  <view class="weui-cells weui-cells_after-title">
    <view class="weui-cell">
      <view class="weui-cell__bd">订单编号：<text class='text-grey'>{{shuju["<hbCode>k__BackingField"]}}</text></view>
    </view>
    <view class="weui-cell">
      <view class="weui-cell__bd">制单人：<text class='text-grey'>{{shuju["<marketMan>k__BackingField"]}}</text></view>
    </view>
    <view class="weui-cell">
      <view class="weui-cell__bd">制单时间：<text class='text-grey'>{{shuju["<marketTime>k__BackingField"]}}</text></view>
    </view>
    <view class="weui-cell">
      <view class="weui-cell__bd">客户名称：<text class='text-grey'>{{shuju["<customerName>k__BackingField"]}}</text></view>
    </view>
    <view class="weui-cell">
      <view class="weui-cell__bd">联系电话：<text class='text-grey'>{{shuju["<cusPhone>k__BackingField"]}}</text></view>
    </view>
    <view class="weui-cell">
      <view class="weui-cell__bd">收货人：<text class='text-grey'>{{shuju["<shouHuoMan>k__BackingField"]}}</text></view>
    </view>
    <view class="weui-cell">
      <view class="weui-cell__bd">收货日期：<text class='text-grey'>{{shuju["<shDate>k__BackingField"]}}</text></view>
    </view>
    <view class="weui-cell">
      <view class="weui-cell__bd">收货地址：<text class='text-grey'>{{shuju["<shAddress>k__BackingField"]}}</text></view>
    </view>
    <view class="weui-cell">
      <view class="weui-cell__bd">物流编号：<text class='text-grey'>{{shuju["<wlNo>k__BackingField"]}}</text></view>
    </view>
    <view class="weui-cell">
      <view class="weui-cell__bd">物流名称：<text class='text-grey'>{{shuju["<wlName>k__BackingField"]}}</text></view>
    </view>
    <view class="weui-cell">
      <view class="weui-cell__bd">备注：<text class='text-grey'>{{shuju["<remark>k__BackingField"]}}</text></view>
    </view>
  </view>
</view>

<view class="weui-panel__hd">产品列表</view>


<view class='contentView' wx:for='{{liebiao}}'>
  <view class='contentView02_1'>
    商品名称:<text class='text-grey'>{{item["<proName>k__BackingField"]}}</text>
  </view>
  <view class='contentView02_1'>
    订购数量:<text class='text-grey'>{{item["<dgNum>k__BackingField"]}}</text>
  </view>
  <view class='contentView02_1'>
    订购单价:<text class='text-grey'>{{item["<dgprice>k__BackingField"]}}</text>
  </view>
   <view class='contentView02_1'>
    总发货数量:<text class='text-grey'>{{item["<dgNum>k__BackingField"]}}</text>
  </view>
   <view class='contentView02_1'>
    本次发货数量:<text class='text-grey'>{{item["<bSendNum>k__BackingField"]}}</text>
  </view>
   <view class='contentView02_1'>
    出库仓库名称:<text class='text-grey'>{{item["<pbasename>k__BackingField"]}}</text>
  </view>
