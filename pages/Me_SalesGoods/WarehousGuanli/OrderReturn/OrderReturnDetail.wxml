 <cu-custom bgColor="bg-gradual-blue" isBack="{{true}}"><view slot="backText">返回</view><view slot="content">订单退货详情</view></cu-custom> 

<view class='ziview'>
  <view class="weui-cells weui-cells_after-title">
    <view class="weui-cell">
      <view class="weui-cell__bd">订单编号：{{shuju["<orderNo>k__BackingField"]}}</view>
    </view>
    <view class="weui-cell">
      <view class="weui-cell__bd">制单人：{{shuju["<marketMan>k__BackingField"]}}</view>
    </view>
    <view class="weui-cell">
      <view class="weui-cell__bd">制单时间：{{shuju["<marketTime>k__BackingField"]}}</view>
    </view>
    <view class="weui-cell">
      <view class="weui-cell__bd">客户名称：{{shuju["<customerName>k__BackingField"]}}</view>
    </view>
    <view class="weui-cell">
      <view class="weui-cell__bd">联系电话：{{shuju["<cusPhone>k__BackingField"]}}</view>
    </view>
    <view class="weui-cell">
      <view class="weui-cell__bd">入库日期：{{shuju["<thDate>k__BackingField"]}}</view>
    </view>
    <view class="weui-cell">
      <view class="weui-cell__bd">备注：{{shuju["<remark>k__BackingField"]}}</view>
    </view>
  </view>
</view>

<view class="weui-panel__hd">产品列表</view>


<view class='contentView'  wx:for='{{liebiao}}'>
  <view class='contentView02_1'>
    商品名称:{{item["<proName>k__BackingField"]}}
  </view>
  <view class='contentView02_1'>
    进货数量:{{item["<dgNum>k__BackingField"]}}
  </view>
  <view class='contentView02_1'>
    入库仓库:
  </view>
</view>