 <cu-custom bgColor="bg-gradual-blue" isBack="{{true}}"><view slot="backText">返回</view><view slot="content">订单退货详情</view></cu-custom> 

<view class='ziview'>
  <view class="weui-cells weui-cells_after-title">
    <view class="weui-cell">
      <view class="weui-cell__bd fontcss">订单编号：<text class='text-grey'>{{shuju["<orderNo>k__BackingField"]}}</text></view>
    </view>
    <view class="weui-cell">
      <view class="weui-cell__bd fontcss">制单人：<text class='text-grey'>{{shuju["<marketMan>k__BackingField"]}}</text></view>
    </view>
    <view class="weui-cell">
      <view class="weui-cell__bd fontcss">制单时间：<text class='text-grey'>{{shuju["<marketTime>k__BackingField"]}}</text></view>
    </view>
    <view class="weui-cell">
      <view class="weui-cell__bd fontcss">客户名称：<text class='text-grey'>{{shuju["<customerName>k__BackingField"]}}</text></view>
    </view>
    <view class="weui-cell">
      <view class="weui-cell__bd fontcss">联系电话：<text class='text-grey'>{{shuju["<cusPhone>k__BackingField"]}}</text></view>
    </view>
    <view class="weui-cell">
      <view class="weui-cell__bd fontcss">入库日期：<text class='text-grey'>{{shuju["<thDate>k__BackingField"]}}</text></view>
    </view>
    <view class="weui-cell">
      <view class="weui-cell__bd fontcss">备注：<text class='text-grey'>{{shuju["<remark>k__BackingField"]}}</text></view>
    </view>
  </view>
</view>

<view class="weui-panel__hd">产品列表</view>


<view class='contentView'  wx:for='{{liebiao}}'>
  <view class='contentView02_1 fontcss01'>
    商品名称:<text class='text-grey'>{{item["<proName>k__BackingField"]}}</text>
  </view>
  <view class='contentView02_1 fontcss01'>
    进货数量:<text class='text-grey'>{{item["<dgNum>k__BackingField"]}}</text>
  </view>
  <view class='contentView02_1 fontcss01'>
    入库仓库:
  </view>
</view>